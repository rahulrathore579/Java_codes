package java_course;
import java.util.*;
public class check_password {
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int p=2426;
	int pass;
	do {
		System.out.println("enter password==");
		pass=sc.nextInt();
	}while(p!=pass);
	System.out.println("Welcome your password is correct...");

}
}
