package java_course;
import java.util.*;
public class Fact_using_recorsion {
	public static void main(String [] args) {
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		System.out.println(fact(num));
	}
	public static long fact(int num) {
		if(num==1) {
			return 1;
		}
		return num*fact(num-1);
	}
}
